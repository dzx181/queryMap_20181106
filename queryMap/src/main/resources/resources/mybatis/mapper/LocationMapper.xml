<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.LocationMapper">

    <insert id="insertLocation" parameterType="com.example.demo.entity.Location">
        INSERT INTO
        line_location(N,O,Lat,Lng,BusLineID)
        VALUES
        (#{N},#{O},#{lat},#{lng},#{busLineID});
    </insert>
</mapper>